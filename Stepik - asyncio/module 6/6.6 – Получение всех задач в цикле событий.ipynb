    {
      "id": "bccb5d13",
      "cell_type": "code",
      "metadata": {},
      "execution_count": null,
      "source": "import asyncio\n\nasync def job(i):\n    await asyncio.sleep(1)\n    print(i)\n\nasync def main():\n    tasks = [asyncio.create_task(job(i)) for i in range(3)]\n    print(len(asyncio.all_tasks()))\n    await asyncio.gather(*tasks)\n\nasyncio.run(main())",
      "outputs": []
    }
