      "cell_type": "code",
      "metadata": {},
      "execution_count": null,
      "source": "import asyncio\n\nasync def worker():\n    t = asyncio.current_task()\n    print(t.get_name())\n    t.set_name('new_name')\n    print(t.get_name())\n\nasync def main():\n    await asyncio.create_task(worker(), name='initial')\n\nasyncio.run(main())",
      "outputs": []
    }
