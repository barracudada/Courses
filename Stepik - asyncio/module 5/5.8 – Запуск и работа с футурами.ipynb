    {
      "id": "8357443f",
      "cell_type": "code",
      "metadata": {},
      "execution_count": null,
      "source": "import asyncio\n\nasync def my_coroutine(i):\n    await asyncio.sleep(1)\n    print(f'Coroutine {i} finished')\n\nasync def main():\n    tasks = [asyncio.ensure_future(my_coroutine(i)) for i in range(3)]\n    await asyncio.gather(*tasks)\n\nasyncio.run(main())",
      "outputs": []
    }
